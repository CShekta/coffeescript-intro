<div class="container row col-md-6 col-md-offset-3">
  <h1 class="center"> Hello, {{ name }} </h1>
</div>

<div class="container row col-md-4 col-md-offset-4">
  <h4>Other Names Entered:</h4>
  <ul class='list-group'>
    {{#each all_names}}
    <li class="list-group-item">
      <span class="label label-default label-pill pull-xs-right">{{this}}</span>
      {{@key}}
      <!-- <button type="delete" action="/deletename" class="btn btn-danger">Delete</button> -->
    </li>
    {{/each}}
  </ul>
</div>
